{
  "name": "@mcp/core",
  "version": "1.0.5",
  "description": "MCP server for browser automation using browserbase",
  "author": "Browserbase, Inc. (https://browserbase.com)",
  "homepage": "https://browserbase.com",
  "type": "module",
  "main": "./cli.js",
  "files": [
    "dist",
    "cli.js",
    "index.d.ts",
    "index.js",
    "config.d.ts",
    "config.js"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js && shx chmod +x cli.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "test-local": "npm pack && npm install -g $(pwd)/$(ls -t *.tgz | head -1) && mcp-server-browserbase",
    "start": "node cli.js",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "default": "./cli.js"
    }
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.5.0",
    "@modelcontextprotocol/sdk": "^1.11.3",
    "commander": "^12.0.0",
    "playwright": "~1.52.0",
    "playwright-core": "~1.52.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^20.10.4",
    "eslint": "^8.57.0",
    "shx": "^0.3.4",
    "typescript": "^5.3.3"
  },
  "bin": {
    "mcp-server-browserbase": "cli.js"
  },
  "publishConfig": {
    "access": "public"
  }
}