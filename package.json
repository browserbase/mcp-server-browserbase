{
  "name": "mcp-server-browserbase-monorepo",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @mcp/core build",
    "build:remote": "pnpm --filter @mcp/remote build",
    "build:stagehand": "pnpm --filter @mcp/stagehand build",
    "dev": "pnpm -r --parallel dev",
    "dev:remote": "pnpm --filter @mcp/remote dev",
    "dev:stagehand": "pnpm --filter @mcp/stagehand dev",
    "lint": "pnpm -r lint"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/webpack": "^5.28.5",
    "pnpm": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "next": "15.2.4"
  }
}